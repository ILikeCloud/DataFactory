{
	"name": "datasource_adls2_delta",
	"properties": {
		"linkedServiceName": {
			"referenceName": "DS_ADLS2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"table_name": {
				"type": "string"
			},
			"table_catalog_name": {
				"type": "string"
			},
			"directory": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{item().dst_table}/@{item().dst_table}_@{formatDateTime(utcnow(),'yyyy')}@{formatDateTime(utcnow(),'MM')}@{formatDateTime(utcnow(),'dd')}@{formatDateTime(utcnow(),'HH')}@{formatDateTime(utcnow(),'mm')}@{formatDateTime(utcnow(),'ss')}.parquet",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().directory,'/',dataset().table_catalog_name)",
					"type": "Expression"
				},
				"fileSystem": "landing"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}